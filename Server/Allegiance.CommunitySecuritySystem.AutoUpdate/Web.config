    <validation validateIntegratedModeConfiguration="false"/>
    <handlers>
      <add name="AutoUpdateHandler" path="*" verb="*" type="Allegiance.CommunitySecuritySystem.AutoUpdate.AutoUpdateHandler" resourceType="Unspecified" preCondition="integratedMode"/>
    </handlers>
  </system.webServer>
</configuration>